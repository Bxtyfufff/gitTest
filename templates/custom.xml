<templateSet group="custom">
  <template name="cs" value="public static final String $VAR$ = &quot;$VAR$&quot;;" description="constant string" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ci" value="public static final int $VAR$ = $VALUE$;&#10;" description="consant int" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ct" value="public static final $TYPE$ $VAR$ = $VALUE$;" description="constant" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ifle" value="if ($group$ != null &amp;&amp; !$group$.isEmpty()) {&#10;    $content$&#10;}" description="if list is empty" toReformat="false" toShortenFQNames="true">
    <variable name="group" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mdf" value="// modify by @cjx $YEAR$ $TODO$" description="modify by author" toReformat="false" toShortenFQNames="true">
    <variable name="YEAR" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TODO" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="doc" value="/**&#10; * $describe$&#10; *&#10; * @author cjx $YEAR$&#10; */" description="" toReformat="false" toShortenFQNames="true">
    <variable name="describe" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="YEAR" expression="date()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="docc" value="/**-----------------------$VALUE$----------------start*/&#10;&#10;/*------------------------$VALUE$----------------end*/" toReformat="true" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>